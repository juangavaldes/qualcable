<%- contentFor('hero') %>
<div class-'jumbotron text-center'>
	<h1 align="center">Address Line Details</h1>
	<div class="row">
	<div class="col-sm-6 col-sm-offset-3">
		<h2>Add new User</h2>

		<form name="addUser" action="/users" method="POST">
			<div class="form-group">
				<label>First Name</label>
				<input type="text" name="firstName" class="form-control" placeholder="ex. John">
			</div>
			<div class="form-group">
				<label>Last Name</label>
				<input type="text" name="lastName" class="form-control" placeholder="ex. Doe">
			</div>
			<div class="form-group">
				<label>Email</label>
				<input type="text" name="lastName" class="form-control" placeholder="ex. jdoe@company.com">
			</div>
			<div class="form-group">
				<label>Username</label>
				<input type="text" name="lastName" class="form-control" placeholder="ex. jdoe">
			</div>
			<div class="form-group">
				<label>Pasword</label>
				<input type="password" name="password" class="form-control" placeholder="ex. pass123456">
			</div>
			<div class="form-group text-right">
				<button type="submit" class="btn btn-primary btn-lng">Add New User</button>
			</div>
		</form>
	</div>
	</div>
</div>