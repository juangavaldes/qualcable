<%- contentFor('hero') %>
<div class-'jumbotron text-center'>
	<h1 align="center">Address Line Details</h1>
	<div class="row">
	<div class="col-sm-6 col-sm-offset-3">
		<h2>Add new Price</h2>
		<form name="addPrice" action="/price" method="POST">
			<div class="form-group">
				<label>Speed</label>
				<input type="text" name="speed" class="form-control">
			</div>
			<div class="form-group">
				<label>Price</label>
				<input type="text" name="price" class="form-control">
			</div>			
			<div class="form-group text-right">
				<button type="submit" class="btn btn-primary btn-lng">Add</button>
			</div>
		</form>
	</div>
	</div>
</div>