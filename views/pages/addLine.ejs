<%- contentFor('hero') %>
<div class-'jumbotron text-center'>
	<h1 align="center">Address Line Details</h1>
	<div class="row">
	<div class="col-sm-6 col-sm-offset-3">
		<h2>Enter Client Address</h2>

		<form action="/addLine" method="POST">
			<div class="form-group">
				<label>Address Line 1</label>
				<input type="text" name="addLine1" class="form-control" placeholder="ex. South St. Alamo">
			</div>
			<div class="form-group">
				<label>Address Line 2</label>
				<input type="text" name="addLine2" class="form-control" placeholder="ex. Apt#">
			</div>
			<div class="form-group">
				<label>State</label>
				<select name="state" class="form-control" placeholder="ex. Florida">
					<option>Chicago</option>
					<option>Florida</option>
				</select>
			</div>
			<div class="form-group">
				<label>City</label>
				<select name="city" class="form-control" placeholder="ex. Doral">
					<option>Doral</option>
					<option>Miami</option>
				</select>
			</div>
			<div class="form-group">
				<label>ZIP Code</label>
				<input type="text" name="zipcode" class="form-control" placeholder="ex. 41225">
			</div>
			<div class="form-group text-right">
				<button type="submit" class="btn btn-primary btn-lng">Search</button>
			</div>
		</form>
	</div>
	</div>
</div>